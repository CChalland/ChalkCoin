    "react-copy-to-clipboard": "5.0.3",
    "react-datetime": "3.0.4",
    "react-dom": "17.0.1",
    "react-jvectormap": "0.0.16",
    "react-notification-alert": "0.0.13",
    "react-select": "3.2.0",
    "react-table": "7.6.3",
    "reactstrap": "8.9.0",
    "request": "^2.88.0",
