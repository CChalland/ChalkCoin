\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t</div>\n\t\t\t\t\t</Col>\n\t\t\t\t</Row>\n\t\t\t\t<Row className=\"mt-4 list-inline-item\">\n\t\t\t\t\t<p>\n\t\t\t\t\t\t<strong className=\"h6\">LAST PLAY</strong>\n\t\t\t\t\t\t{`: ${situation.lastPlay.text}`}\n\t\t\t\t\t</p>\n\t\t\t\t</Row>\n\t\t\t</Container>\n\t\t);\n\t};\n\n\tconst footballHelper = (situation) => {\n\t\tlet driveContent = document.querySelector(\".sb-detail\");\n\t\tlet sbDetailWidth = driveContent?.clientWidth;\n\t\tlet markerListWidth = 17 * (sbDetailWidth / 210);\n\n\t\tlet progressBar = document.querySelector(\".progress-wrap\");\n\t\tlet progressBarWidth = progressBar?.clientWidth / 12;\n\t\tlet markerWidth = progressBarWidth / 2;\n\n\t\treturn (\n\t\t\t<Container fluid>\n\t\t\t\t<section className=\"sb-detail\">\n\t\t\t\t\t<div className=\"football-drivechart play-by-play players\">\n\t\t\t\t\t\t<div className=\"drivechart\">\n\t\t\t\t\t\t\t<span className=\"bar\"></span>\n\t\t\t\t\t\t\t<div className=\"progress-wrap\">\n\t\t\t\t\t\t\t\t<span className=\"progress\" style={{ left: \"1%\", right: \"1%\" }}></span>\n\t\t\t\t\t\t\t\t<div className=\"logo-wrap home\" style={{ left: \"55%\" }}>\n\t\t\t\t\t\t\t\t\t<div className=\"logo\">\n\t\t\t\t\t\t\t\t\t\t<div className=\"front\">\n\t\t\t\t\t\t\t\t\t\t\t<img\n\t\t\t\t\t\t\t\t\t\t\t\tclassName=\"focus-image scoreboard-logo-home imageLoaded\"\n\t\t\t\t\t\t\t\t\t\t\t\tsrc=\"https://a.espncdn.com/combiner/i?img=/i/teamlogos/nfl/500/scoreboard/chi.png&amp;w=35&amp;h=35\"\n\t\t\t\t\t\t\t\t\t\t\t/>\n\t\t\t\t\t\t\t\t\t\t\t<div className=\"logo-placeholder\">CHI</div>\n\t\t\t\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t\t\t\t<div className=\"back\">\n\t\t\t\t\t\t\t\t\t\t\t<img\n\t\t\t\t\t\t\t\t\t\t\t\tclassName=\"focus-image scoreboard-logo-away imageLoaded\"\n\t\t\t\t\t\t\t\t\t\t\t\tsrc=\"https://a.espncdn.com/combiner/i?img=/i/teamlogos/nfl/500/scoreboard/buf.png&amp;w=35&amp;h=35\"\n\t\t\t\t\t\t\t\t\t\t\t/>\n\t\t\t\t\t\t\t\t\t\t\t<div className=\"logo-placeholder\">BUF</div