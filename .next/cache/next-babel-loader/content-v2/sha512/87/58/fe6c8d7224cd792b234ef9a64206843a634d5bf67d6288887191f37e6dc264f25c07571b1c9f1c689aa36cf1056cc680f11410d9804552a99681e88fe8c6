n  };\n}","map":{"version":3,"sources":["/Users/colechalland/Projects/ChalkCoin/helpers/BetCard.js"],"names":["capitalize","firstLetter","restOfWord","toUpperCase","join","homeAwayHelper","game","homeTeam","competitions","competitors","filter","team","homeAway","awayTeam","oddsHelper","outcomes","teamPick","vigProbability","odd","find","outcome","name","includes","shortDisplayName","price","marketHelper","market","away","home","awayOdds","homeOdds","totalProb","winProbability","key","BetGameData","bet","currentUserId","spreadPercentages","nfl","favorite","underdog","ncaaf","nba","ncaab","event","homeRecords","homeAvgProb","awayRecords","awayAvgProb","displayedWinner","temp","type","summary","bookmakers","odds","map","title","markets","displayName","awayTeamOdds","winPercentage","homeTeamOdds","moneyLine","favoriteTeam","favoriteValue","details","split","favoritePercentage","underdogPercentage","Math","abs","abbreviation","sport","provider","reduce","total","bookmaker","index","array","length","toFixed","records","alternateColor","color","logo","score","parseInt","requesterTeam","winner","winProb","requesterId","id","amount","date","sportName","status","shortDetail","detail","openStatus","venue"],"mappings":";;;;;;AAAA,MAAMA,UAAU,GAAG,CAAC,CAACC,WAAD,EAAc,GAAGC,UAAjB,CAAD,KAAkCD,WAAW,CAACE,WAAZ,KAA4BD,UAAU,CAACE,IAAX,CAAgB,EAAhB,CAAjF;;AACA,SAASC,cAAT,CAAwBC,IAAxB,EAA8B;AAC7B,MAAIC,QAAQ,GAAGD,IAAI,CAACE,YAAL,CAAkB,CAAlB,EAAqBC,WAArB,CAAiCC,MAAjC,CAAyCC,IAAD,IAAU;AAChE,WAAOA,IAAI,CAACC,QAAL,KAAkB,MAAzB;AACA,GAFc,CAAf;AAGA,MAAIC,QAAQ,GAAGP,IAAI,CAACE,YAAL,CAAkB,CAAlB,EAAqBC,WAArB,CAAiCC,MAAjC,CAAyCC,IAAD,IAAU;AAChE,WAAOA,IAAI,CAACC,QAAL,KAAkB,MAAzB;AACA,GAFc,CAAf;AAGA,SAAO;AAAEL,IAAAA,QAAF;AAAYM,IAAAA;AAAZ,GAAP;AACA;;AACD,SAASC,UAAT,CAAoBC,QAApB,EAA8BC,QAA9B,EAAwC;AACvC,MAAIC,cAAJ;AACA,MAAIC,GAAG,GAAGH,QAAQ,CAACI,IAAT,CAAeC,OAAD,IAAaA,OAAO,CAACC,IAAR,CAAaC,QAAb,CAAsBN,QAAQ,CAACL,IAAT,CAAcY,gBAApC,CAA3B,CAAV;;AACA,MAAIL,GAAG,CAACM,KAAJ,GAAY,CAAhB,EAAmB;AAClBP,IAAAA,cAAc